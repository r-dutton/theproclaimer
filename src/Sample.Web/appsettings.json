{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Reports": "Data Source=reports.db"
  },
  "Downstream": {
    "Reports": {
      "BaseUrl": "https://localhost:6001"
    }
  },
  "Retention": {
    "RetentionDays": 45,
    "PollingIntervalMinutes": 30,
    "CachePrefix": "report",
    "CacheExpirationMinutes": 10
  },
  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://local/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA="
  }
}
